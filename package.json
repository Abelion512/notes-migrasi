{
  "name": "lembaran",
  "version": "3.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "bin": {
    "lembaran": "./packages/cli/src/main.ts"
  },
  "scripts": {
    "dev": "bun --filter @lembaran/web dev",
    "build": "bun --filter @lembaran/web build",
    "start": "bun --filter @lembaran/web start",
    "cli": "bun --filter @lembaran/cli start",
    "lint": "bun --filter \"*\" lint",
    "test:perf": "bun packages/core/src/scripts/inject-perf-data.ts",
    "sinkron-aset": "bun --filter @lembaran/web sinkron-aset"
  },
  "dependencies": {
    "commander": "^14.0.3",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "typescript": "^5",
    "eslint": "^9"
  }
}
